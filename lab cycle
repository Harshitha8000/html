<!DOCTYPE html>

<html>

<head>
    <style>
        p {
            color: red;
            font-weight: bold;
            font-size: 50;
        }
    </style>
    <script language="javascript">
        function capital() {
            var country = document.forms["capitalform"].Country.value;
            var capital = " Please select any country ";
            if (country == "India") {
                Capital = "New Delhi";
            }
            if (country == "Japan") {
                Capital = "Tokyo";
            }
            if (country == "South Korea") {
                Capital = "seoul";
            }
            if (country == "USA") {
                Capital = "Washington DC";
            }
            if (country == "Canada") {
                Capital = "Torento";
            }
            document.getElementById("Capital").innerHTML = Capital;
        }
    </script>
</head>

<body>
    <form name="capitalform">
        <br />
            Select a Country : <select name="Country" onchange="capital()">
                <option value="India">India</option>
                <option value="Japan">Japan</option>
                <option value="South Korea">South Korea</option>
                <option value="USA">USA</option>
                <option value="Canada">Canada</option>
            </select>
            <br />
            <br />
            <font color="Red" size="6">Capital is :</font>
            <p id="Capital"></p>
    </form>
</body>

</html>


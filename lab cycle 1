file:lab_108a.html
-------------------------
<!DOCTYPE html>
<html>
    <head>
        <title>web page</title>
    </head>
    <body>
        <form action="/action_page.php">
            <label for="fname">First name:</label><br>
            <input type="text" id="fname" name="fname"><br>
            <label for="email">Enter your email:</label><br>
            <input type="email" id="email" name="email"><br>

            <label for="pwd">Password:</label><br>
            <input type="password" id="pwd" name="pwd"><br>

            <label for="phone">Enter your phone number:</label><br>
            <input type="tel" id="phone" name="phone" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}"><br>

            <p> Enter Language:</p>
            <input type="checkbox" id="telugu" name="telugu" >
            <label for="telugu"> Telugu</label><br>
            <input type="checkbox" id="hindi" name="hindi" >
            <label for="hindi"> Hindi</label><br>
            <input type="checkbox" id="english" name="english" >
            <label for="english"> English</label><br>
            
            <p> Age :</p>
            <input type="radio" id=" minor" name="ageGroup" value=" minor">
            <label for=" minor">  0-18 </label><br>
            <input type="radio" id=" major" name="ageGroup" value=" major">
            <label for=" major"> 18-60</label><br>
            <input type="radio" id=" old" name="ageGroup" value=" old">
            <label for=" old"> 18 above</label><br>

            <br>
            <label for="birthday">Birthday:</label>
            <input type="date" id="birthday" name="birthday"><br>
            
            <br><br><input type="submit" value="Submit">
            <input type="reset"><br>
          </form>
    </body>
</html>

=========================
file:lab_108b.html
-------------------------
<!DOCTYPE html>
<html>
    <head>
        <title>web page</title>
    </head>
    <form>
        <p> Select a color :</p>
        <input type="radio" onclick="myFunction('blue')" id="blue" name="colors" value=" blue">
        <label for=" blue"> blue </label><br>
        <input type="radio" onclick="myFunction('green')" id="green" name="colors" value=" green">
        <label for=" green"> green</label><br>
        <input type="radio" onclick="myFunction('red')" id="red" name="colors" value=" red">
        <label for=" red"> red</label><br>
        <input type="radio" onclick="myFunction('yellow')" id="yellow" name="colors" value=" yellow">
        <label for=" yellow"> yellow</label><br>
        <input type="radio" onclick="myFunction('pink')" id="pink" name="colors" value=" pink">
        <label for=" pink"> pink</label><br>
    </form>
    <script>
        function myFunction(col) {
            document.getElementsByTagName("BODY")[0].style.backgroundColor = col;
        }
    </script>
</html>

